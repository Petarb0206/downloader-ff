{

    "manifest_version": 2,
    "name": "Downloader",
    "version": "1.0",

    "description": "Downloads youtube videos.",

    "browser_specific_settings": {
        "gecko": {
          "id": "downloader@petar.com",
        }
    },

    "browser action": {
      "default_icon": "icon/icon.png"
    },

    "permissions": ["tabs","nativeMessaging"],


    "background": {
        "scripts": ["download.js"]
    }

}
